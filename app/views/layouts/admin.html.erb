<!DOCTYPE html>
<html>
  <head>
    <title>TestVentura</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <%= stylesheet_link_tag    'admin', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if check_active_controller(["sessions"]) %>
      <%= yield %>
    <% else %>
    <%= render "/admins/shared/navbar_left"%>
      <div class="container">
        <div class="conten">
          <%= yield %>
        </div>
      </div>
    <% end %>
  </body>
</html>

<%= simple_form_for([:admins, @job]) do |f|%>
	<%= f.input :name , label: "Title job"%>
	<%= f.input :price , label: "Salary"%>
	<%= f.input :company%>
	<%= f.input :description %>
	<%= f.input :photo, accept: "image/png,image/gif,image/jpeg" %>
	<%= f.input :industry_id,  collection: Industry.all.map { |c| [c.title, c.id]}, input_html: {class: "select2"} ,require:'true'%>
	<%= f.submit "Save", class: 'btn btn-devise' %>
<% end %>